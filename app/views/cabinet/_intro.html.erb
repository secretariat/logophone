<%= form_tag cabinet_intro_show_path(:quantity => @quantity), :method => :get, :remote => true do %>
	<%= text_field_tag :phone, @number[1], :class => "minput_small", :maxlenght => "1", :readonly => true %>
	<%= text_field_tag :phone, @number[2], :class => "minput_small", :maxlenght => "1", :readonly => true %>
