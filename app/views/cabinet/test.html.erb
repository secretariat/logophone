<article class="element fx-slide-up home clearfix one-full about isotope-item">
  <div class="reveal-bg-nine">
    <center><h2 class="pad pad-top-minus pad-bottom-minus"><%= t 'test.title' %></h2></center>
  </div>
</article>
<article class="element clearfix home pp one-full">
	<% if @theme.present? && @theme == "dark" %>
		<h4 class="introtext lt"><%= t 'test.header' %></h4>
		<h3 class="lt" style="float:left;padding:0px 20px 0px 210px;"><%= t 'test.phrase' %></h3>
	<% else %>
		<h4 class="introtext"><%= t 'test.header' %></h4>
		<h3 style="float:left;padding:0px 20px 0px 210px;"><%= t 'test.phrase' %></h3>
	<% end %>
		<%= form_tag( cabinet_test_show_path, :method => "get", :style =>"padding:18px 20px;") do %>
			<%= select_tag "time", options_for_select([5,4,3,2,1,0.9,0.8,0.7,0.6,0.5,0.4,0.3,0.2]) %>
			<%= submit_tag("#{t 'buttons.create'}", :class => "msubmit") %>
		<% end %>
</article>
