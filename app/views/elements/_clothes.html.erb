      <div class="item">
          <%= link_to(image_tag('cloth/m9.png'), '/assets/cloth/9.png', {:class => "popup"})%>
      </div>
    </div>
    <!-- Carousel nav -->
    <a class="carousel-control left" href="#carousel-04" data-slide="prev">
      <%= image_tag("left.png")%>
    </a>
    <a class="carousel-control right" href="#carousel-04" data-slide="next">
      <%= image_tag("right.png")%>
    </a>
  </div>
  <!-- Carousel ends -->

  <div class="inner-panel reveal-bg-three">
    <div class="thumb pad pad-bottom-minus">
      <%#= image_tag("slider.png")%>
    </div>
    <h2 class="pad pad-top-minus">
      <center>
